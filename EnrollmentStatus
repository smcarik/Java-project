/**
 * sets the enrollment status of students  **/ public enum EnrollmentStatus{
  success("Enrollment successful"), secFull("Enrollment Failed: Section is full"),preReq("Enrollment Failed: prerequisites are not satisfied. "),prevEnroll("Enrollment Failed: Student was Previously Enrolled.");
  
  private String value; //Enrollment Status value
  /**
   * Sets value to given value
   * @param value to be given
   */
  EnrollmentStatus(String value){
    this.value = value;
  }
  /**
   * Gets the value of enrollment status
   * @return value of enrollment status
   */
  public String getValue(){
    return value;
  }
}
