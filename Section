  {
    if(CourseCatalog.isEmpty() == false)
    {
      return false;
    }
    else
    {
      return true;
    }
  }
  
  public int getTotalEnrollment()
  {
    return "Something";
  }
  
  public String display()
  {
    return "something";
  }
  
  public String displayStudentRoster()
  {
    return "Someting";
  }
  
  public String getGrade()
  {
    return "Something";
  }
  
